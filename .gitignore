# Basic PHP .gitignore for custom projects

# Environment files (never commit these!)
.env
.env.*
!.env.example

# Dependency directories
/vendor/
/node_modules/

# Log files
*.log
logs/

# Temporary files
cache/
tmp/
*.tmp

# System files
.DS_Store
Thumbs.db

# IDE files
.idea/
.vscode/
*.swp
*.swo

# Local configuration files (create these from examples)
config.php
database.php

# OS generated files
ehthumbs.db
Folder.jpg
